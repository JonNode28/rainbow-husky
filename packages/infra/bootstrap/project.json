{
  "name": "infra-bootstrap",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/infra/bootstrap/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/infra/bootstrap",
        "main": "packages/infra/bootstrap/src/index.ts",
        "tsConfig": "packages/infra/bootstrap/tsconfig.lib.json",
        "assets": ["packages/infra/bootstrap/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/infra/bootstrap/**/*.ts"]
      }
    }
  },
  "tags": []
}
